{
  "name": "json-pascal-case",
  "version": "0.0.2",
  "description": "A simple JSON parser that converts keys to pascal case",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/json-pascal-case.git"
  },
  "keywords": [
    "json",
    "pascal",
    "case",
    "parser",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/json-pascal-case/issues"
  },
  "homepage": "https://github.com/hebertcisco/json-pascal-case#readme",
  "devDependencies": {
    "@types/jest": "29.2.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.23.1",
    "eslint-plugin-jest": "27.1.3",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "pascal-case": "^3.1.2"
  }
}